{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-electron"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico" 
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "themes/css/material-design-iconic-font/dist/css/material-design-iconic-font.min.scss",
        "themes/css/animate.css/animate.min.scss",
        "themes/css/jquery.scrollbar/jquery.scrollbar.scss",
        "themes/css/select2/dist/css/select2.min.css",
        "themes/css/dropzone/dist/dropzone.css",
        "themes/css/flatpickr/dist/flatpickr.min.css",
        "themes/css/nouislider/distribute/nouislider.min.css",
        "themes/css/bootstrap-colorpicker/dist/css/bootstrap-colorpicker.css",
        "themes/css/trumbowyg/dist/ui/trumbowyg.min.css",
        "themes/css/rateYo/min/jquery.rateyo.min.css",
        "themes/css/app.min.scss"
      ],
      "scripts": [
      	"themes/js/jquery/dist/jquery.min.js",
      	"themes/js/popper.js/dist/umd/popper.min.js",
      	"themes/js/bootstrap/dist/js/bootstrap.min.js",
      	"themes/js/jquery.scrollbar/jquery.scrollbar.min.js",
      	"themes/js/jquery-scrollLock/jquery-scrollLock.min.js",
        "themes/js/remarkable-bootstrap-notify/dist/bootstrap-notify.min.js",
        "themes/js/jquery-mask-plugin/dist/jquery.mask.min.js",
        "themes/js/select2/dist/js/select2.full.min.js",
        "themes/js/dropzone/dist/min/dropzone.min.js",
        "themes/js/moment/min/moment.min.js",
        "themes/js/flatpickr/dist/flatpickr.min.js",
        "themes/js/nouislider/distribute/nouislider.min.js",
        "themes/js/bootstrap-colorpicker/dist/js/bootstrap-colorpicker.min.js",
        "themes/js/trumbowyg/dist/trumbowyg.min.js",
        "themes/js/rateYo/min/jquery.rateyo.min.js",
        "themes/js/jquery-text-counter/textcounter.min.js",
        "themes/js/autosize/dist/autosize.min.js",
      	"themes/js/app.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
